    <formulas>
        <description>Populates as the Account or Contact Phone depending on the Meeting Master record</description>
        <name>AccountOrContactPhone</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!ContactMobilePhone})), {!ContactMobilePhone}, {!AccountPhone})</expression>
    </formulas>
    <formulas>
        <description>Record ID of the newly created Meeting Cadence against the user&apos;s business unit</description>
        <name>BU_NewOrFoundMeetingCadence</name>
        <dataType>String</dataType>
        <expression>IF(NOT(ISBLANK({!BU_NewMeetingCadenceId})), {!BU_NewMeetingCadenceId}, {!Get_Meeting_Cadence_Business_Unit_Record_Data.Id})</expression>
    </formulas>
    <formulas>
        <description>Is the Meeting associated primarily with the Contact or is it associated with the Account&apos;s Contact</description>
        <name>BU_PrimaryContactOrAcountContact</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!AccountContactId}), {!MeetingContact_0.recordId}, {!AccountContactId})</expression>
    </formulas>
    <formulas>
        <description>Meeting End Date/Time calculated from Start Date/Time + Duration</description>
        <name>MeetingEndDateTime</name>
        <dataType>DateTime</dataType>
        <expression>IF(ISPICKVAL({!Get_MEETabl_General_Settings_Master_CMDT.Meetings_Assigned_To__c},&quot;User&quot;), ({!Start_Date_Time} + (VALUE(IF({!Duration_minutes} = &quot;Half Day&quot;, &quot;240&quot;, IF({!Duration_minutes} = &quot;Full Day&quot;, &quot;480&quot;,{!Duration_minutes})))/1440)),
IF(ISPICKVAL({!Get_MEETabl_General_Settings_Master_CMDT.Meetings_Assigned_To__c},&quot;Business Unit&quot;), ({!Start_Date_Time_0} + (VALUE(IF({!Duration_minutes_0} = &quot;Half Day&quot;, &quot;240&quot;,IF({!Duration_minutes_0}=&quot;Full Day&quot;, &quot;480&quot;,{!Duration_minutes_0})))/1440)),
null))</expression>
    </formulas>
    <formulas>
        <description>Meeting Location associated with the Meeting created. The location is used when creating an Event</description>
        <name>MeetingLocation</name>
        <dataType>String</dataType>
        <expression>IF({!User},
IF(ISBLANK({!Location.street}), &quot;&quot; , {!Location.street} + &quot;, &quot;) + IF(ISBLANK({!Location.city}), &quot;&quot;,{!Location.city}+&quot; &quot;) +  IF(ISBLANK({!Location.province}),&quot;&quot;, {!Location.province} + &quot; &quot;) + IF(ISBLANK({!Location.postalCode}), &quot;&quot;,{!Location.postalCode} +&quot;, &quot;) + IF(ISBLANK({!Location.country}), &quot;&quot;,{!Location.country}),

IF({!Business_Unit},
IF(ISBLANK({!Location_0.street}), &quot;&quot;,{!Location_0.street} + &quot;, &quot;) + IF(ISBLANK({!Location_0.city}), &quot;&quot;,{!Location_0.city}+&quot; &quot;) +  IF(ISBLANK({!Location_0.province}), &quot;&quot;,{!Location_0.province} + &quot; &quot;) + IF(ISBLANK({!Location_0.postalCode}), &quot;&quot;,{!Location_0.postalCode} +&quot;, &quot;) + IF(ISBLANK({!Location_0.country}), &quot;&quot;,{!Location_0.country}),
&quot;&quot;))</expression>
    </formulas>
    <formulas>
        <description>Formula evaluates to the nearest half hour based on the current date/time</description>
        <name>NearestHalfHour</name>
        <dataType>DateTime</dataType>
        <expression>DATETIMEVALUE(
 LEFT(TEXT(Now()),11) 
 + LPAD(
   TEXT( VALUE( MID(TEXT(Now()),12,2) ) 
    + IF( VALUE(MID(TEXT(Now()),15,2)) &gt; 44, 1, 0)
    ), 2, &quot;0&quot;) 
 +
     IF( VALUE( MID(TEXT(Now()),15,2)) &lt;30, &quot;:30&quot;,
       &quot;:00&quot;)
 + &quot;:00Z&quot;
)</expression>
    </formulas>
    <formulas>
        <description>Is the Account record the primary meeting account or the related contact&apos;s account</description>
        <name>PrimaryAccountorContactAccount</name>
        <dataType>String</dataType>
        <expression>IF(ISBLANK({!ContactAccountId}) &amp;&amp; ISPICKVAL({!Get_MEETabl_General_Settings_Master_CMDT.Parent_Record__c},&quot;Contact&quot;), &quot;&quot;,
IF(NOT(ISBLANK({!ContactAccountId})), {!ContactAccountId},{!MeetingMasterRecordId}))</expression>
    </formulas>
    <formulas>
        <description>Evaluates to either the newly created Meeting Cadence from the first time the flow is initiated OR from a found Meeting Cadence record for the user</description>
        <name>User_NewOrFoundMeetingCadence</name>
